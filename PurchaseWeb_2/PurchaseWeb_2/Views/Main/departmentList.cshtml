@model IEnumerable<PurchaseWeb_2.ModelData.Department_mst>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Department_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Create_date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Department_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Create_date)
        </td>
        <td>
            @Html.ActionLink("Edit", "departmentEdit", new { id=item.Dpt_id }) |
            @Html.ActionLink("Delete", "departmentDelete", new { id=item.Dpt_id })
        </td>
    </tr>
}

</table>
