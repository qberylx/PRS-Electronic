@model PurchaseWeb_2.ModelData.CPRFBudget_Mst

@{
    ViewBag.Title = "ViewCprf";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<div class="row">
    <div class="col-md-8">
        <div id="divCPRFMainForm">
            @{ Html.RenderAction("CPRFMstView", "CPRF", new { CprfId = Model.CPRF_Id });}
        </div>
    </div>
    <div class="col-md-4">
        <div id="divFileForm">
            @{ Html.RenderAction("CPRFFileView", "CPRF", new { CprfId = Model.CPRF_Id });}
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div id="divCPRFDtlsForm">
            @{ Html.RenderAction("CPRFDtlsView", "CPRF", new { CprfId = Model.CPRF_Id, URL = ViewBag.URL });}
        </div>
    </div>
</div>

