@model PurchaseWeb_2.ModelData.CPRFBudget_Mst

@{
    ViewBag.Title = "CPRFFinishing";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<div class="row">
<div class="col-md-8">
    <div id="divCPRFMainForm">
        @{ Html.RenderAction("CPRFFinishingForm", "CPRF", new { CprfId = Model.CPRF_Id });}
    </div>
</div>
<div class="col-md-4">
    <div id="divFileForm">
        @{ Html.RenderAction("CPRFFileForm", "CPRF", new { CprfId = Model.CPRF_Id });}
    </div>
</div>
</div>

<div class="row">
    <div class="col-md-12">
        <div id="divCPRFAssetFinishingForm">
            @{ Html.RenderAction("CPRFAssetFinishingForm", "CPRF", new { CprfId = Model.CPRF_Id });}
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div id="divCPRFDtlsLst">
            @{ Html.RenderAction("CPRFDtlsLstView", "CPRF", new { CprfId = Model.CPRF_Id });}
        </div>
    </div>
</div>
